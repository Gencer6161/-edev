#include <iostream>
#include <stdio.h>
#include<locale.h>
#pragma warning (disable:4996)
using namespace std;

int main() {
int faktoriyel = 1;
int sayi;
cout <<  "faktoriyelini hesaplamak istediginiz sayiyi giriniz\n-->";
cin >> sayi;
for(int i = 1; i <= sayi; i++){
	
	faktoriyel *= i ;
}
cout << faktoriyel;
}
